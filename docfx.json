{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/ivrToolkit.Core.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": ""
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "template": [
      "default",
      "modern"
    ],
    "content": [
      {
        "files": [
          "**/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site/docs"
  }
}
