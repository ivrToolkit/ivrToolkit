{
  "metadata": [
    {
      "src": [
        {
          "files": [ "ivrToolkit.Core/ivrToolkit.Core.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "template": [
      "default"
    ],
    "content": [
      {
        "files": [
          "api/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site/docs"
  }
}
